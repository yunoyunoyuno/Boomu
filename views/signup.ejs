<% include partials/header %>
 
      <div class="container" id = "signup-content">
          <div class="row">
            <div class="col-lg-10">
              <div class="signup-content text-center rounded-lg">
                <h1 class="display-5 text-light authenticate">Sign Up</h1>
                <form class="form">
                  <input type = "text" placeholder = "name" class = "form-control form-control-md mt-4" id = "name" >
                  <input type="text" placeholder="Email" class = "form-control form-control-md" id = "email">
                  <input type="password" placeholder="Password(8-16 characters)" class = 'form-control form-control-md password' id = "password">
                  <input type="password" placeholder="Password Confirm" class = 'form-control form-control-md password' id = "passwordConfirm">
                  <button type = "submit" href="#" class="btn btn-outline-light d-block login-btn shadow-lg mx-auto mt-4">Sign Up</button>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>

  </body>
